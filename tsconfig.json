{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "jest.config.cjs",
    "node_modules"
  ]
}